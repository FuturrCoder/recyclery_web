{
  "version": 2,
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "apps/frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}